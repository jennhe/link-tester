<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>JSP Example Application</display-name>
	<servlet>
		<servlet-name>gpa</servlet-name>
		<servlet-class>com.bt.gpa.GPAServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>about</servlet-name>
		<servlet-class>com.bt.gpa.AboutServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>gpa</servlet-name>
		<url-pattern>/gpa</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>about</servlet-name>
		<url-pattern>/about</url-pattern>
	</servlet-mapping>
</web-app>
